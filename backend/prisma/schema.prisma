generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Country {
  id    Int    @id @default(autoincrement())
  code  String @unique
  users User[]
}

model User {
  id           Int     @id @default(autoincrement())
  email        String  @unique
  passwordHash String
  role         String
  countryId    Int?
  country      Country? @relation(fields: [countryId], references: [id])
}
